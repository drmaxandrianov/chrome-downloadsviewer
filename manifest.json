{
	"manifest_version": 2,
	"name": "Downloads Viewer",
	"version": "0.0.1",
	"icons": {
			"16": "img/icon-16.png",
			"24": "img/icon-24.png",
			"32": "img/icon-32.png",
			"48": "img/icon-48.png",
			"64": "img/icon-64.png",
			"72": "img/icon-72.png",
			"96": "img/icon-96.png",
			"128": "img/icon-128.png",
			"256": "img/icon-256.png"
	},
	"description": "Monitor your downloads from any place on our planet with this small and wonderful extension for Google Chrome.",
	"browser_action": {
		"default_icon": "img/icon-32.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"http://pusher.pixelowner.com/",
		"downloads",
		"<all_urls>"
	]
}
